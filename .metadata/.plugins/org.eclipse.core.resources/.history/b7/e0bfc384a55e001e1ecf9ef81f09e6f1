package Day01;

import java.util.Scanner;

public class Day01 {
	public static void main(String[] args) {
		System.out.println("안녕하세요 저는 이현우 입니다");
		Scanner sc = new Scanner(System.in);
		int user = sc.nextInt();
		if(user != null) {
			for(int i = 0; i < 10; i++) {
				System.out.println(i);
			}			
		}
	}
}
